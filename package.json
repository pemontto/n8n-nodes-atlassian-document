      "dist/nodes/AtlassianDocumentFormatter/AtlassianDocumentFormatter.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.39.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.5",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@atlaskit/editor-json-transformer": "^8.31.2",
    "@atlaskit/editor-markdown-transformer": "^5.20.2",
    "@atlaskit/editor-wikimarkup-transformer": "^11.20.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "packageManager": "pnpm@10.20.0"
}