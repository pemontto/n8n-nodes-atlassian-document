{
  "name": "n8n-nodes-atlassian-document",
  "version": "0.1.0",
  "description": "n8n community node for converting between Atlassian Document Format (ADF), Wiki Markup, and Markdown",
  "license": "MIT",
  "homepage": "",
  "keywords": [
    "n8n-community-node-package",
    "atlassian",
    "adf",
    "document-format",
    "jira",
    "confluence",
    "wiki-markup",
    "markdown"
  ],
  "author": {
    "name": "pemontto",
    "email": "pemontto@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pemontto/n8n-nodes-atlassian-document.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": false,
    "credentials": [],
